{
  "name": "ddd-cqrs-es-aws-sam",
  "version": "0.1.0",
  "description": "Domain Driven Design, CQRS, and Event Sourcing example utilizing AWS SAM.",
  "keywords": [],
  "license": "MIT",
  "author": "Dan Lydiard",
  "scripts": {
    "build:dev": "NODE_ENV=development webpack-cli --progress",
    "build:prod": "NODE_ENV=production webpack-cli --progress -p",
    "clean": "rimraf .aws-sam/",
    "deploy": "sam deploy --template-file template.packaged.yaml --stack-name ddd-cqrs-es-aws-sam --capabilities CAPABILITY_IAM --no-fail-on-empty-changeset --parameter-overrides $(cat .env | tr '\r\n' ' ')",
    "package": "sam package --s3-bucket ddd-cqrs-es-aws-sam --output-template-file template.packaged.yaml",
    "start": "sam local start-api --parameter-overrides $(cat .env | tr '\r\n' ' ')",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "watch": "NODE_ENV=development webpack-cli -w"
  },
  "dependencies": {
    "@node-ts/bus-core": "^0.4.9",
    "@node-ts/ddd": "^0.1.4",
    "@node-ts/logger-core": "^0.1.0",
    "@node-ts/logger-winston": "^0.1.0",
    "aws-sdk": "^2.580.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "http-status-codes": "^1.4.0",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.16",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.35",
    "@types/chai": "^4.1.7",
    "@types/jest": "^24.0.23",
    "@types/mocha": "^5.2.6",
    "@types/node": "^8.10.59",
    "@types/ts-nameof": "^4.2.0",
    "@types/uuid": "^3.4.6",
    "aws-sam-webpack-plugin": "^0.4.0",
    "babel-loader": "^8.3.0",
    "cassava": "^2.5.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "file-loader": "^6.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "jest": "^24.9.0",
    "json-loader": "^0.5.7",
    "long": "^4.0.0",
    "mocha": "^6.0.2",
    "pino-pretty": "^3.4.0",
    "post-compile-webpack-plugin": "^0.1.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.2.0",
    "ts-loader": "^9.4.2",
    "ts-nameof": "^4.2.2",
    "ts-nameof-loader": "^1.0.2",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "typescript": "^3.7.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "webpack-node-externals": "^1.7.2",
    "yaml-cfn": "^0.2.3",
    "zip-webpack-plugin": "^3.0.0"
  }
}
